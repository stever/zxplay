{
  "name": "web",
  "private": true,
  "scripts": {
    "dev": "run-s build:clean:all build:wasm:debug serve",
    "test": "run-s test:jsspeccy test:jest",
    "test:jsspeccy": "node --experimental-wasm-modules test/jsspeccy/test.mjs test/jsspeccy/tests.in test/jsspeccy/tests.expected",
    "test:jest": "jest --passWithNoTests",
    "build": "npm run build:all:release",
    "build:all:debug": "run-s build:clean:all build:copyfiles build:babel build:wasm:debug build:js:debug build:clean",
    "build:all:release": "run-s build:clean:all build:copyfiles build:babel build:wasm:release build:js:release build:clean",
    "build:clean": "del-cli build es5",
    "build:clean:all": "del-cli build es5 public/dist",
    "build:copyfiles": "copyfiles \"src/**/*\" -u 1 es5",
    "build:babel": "babel src --out-dir es5",
    "build:core": "run-s build:mkdir build:gencore",
    "build:mkdir": "mkdir build",
    "build:gencore": "node src/lib/jsspeccy/generator/gencore.mjs src/lib/jsspeccy/generator/core.ts.in build/core.ts",
    "build:wasm:debug": "run-s build:core build:as:debug",
    "build:wasm:release": "run-s build:core build:as:release",
    "build:as:debug": "asc build/core.ts --target debug",
    "build:as:release": "asc build/core.ts --target release",
    "build:js:debug": "webpack",
    "build:js:release": "webpack --env production",
    "watch": "npm-watch",
    "serve": "npx webpack serve --env codespace=${CODESPACE_NAME} --env domain=${GITHUB_CODESPACES_PORT_FORWARDING_DOMAIN}",
    "typedoc:jsspeccy": "typedoc --entryPointStrategy expand --out ./typedocs/jsspeccy ./build/"
  },
  "dependencies": {
    "@lagunovsky/redux-react-router": "^4.5.0",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "file-dialog": "^0.0.8",
    "history": "^5.3.0",
    "jszip": "^3.10.1",
    "pako": "^2.1.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primereact": "^9.6.4",
    "query-string": "^9.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.1",
    "react-titled": "^2.1.0",
    "react-transition-group": "^4.4.5",
    "redux": "^5.0.1",
    "redux-saga": "^1.3.0",
    "sass": "^1.93.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.5",
    "@babel/core": "^7.26.5",
    "@babel/plugin-transform-runtime": "^7.26.5",
    "@babel/preset-env": "^7.26.5",
    "@babel/preset-react": "^7.26.3",
    "assemblyscript": "0.19.6",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "del-cli": "^6.0.0",
    "esbuild": "^0.25.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-transform-stub": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.13.0",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react-test-renderer": "^18.3.1",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "svg-inline-loader": "^0.2.3",
    "typedoc": "^0.27.7",
    "webpack": "^5.97.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  },
  "watch": {
    "build:core": {
      "patterns": [
        "src/lib/jsspeccy/generator/*.js",
        "src/lib/jsspeccy/generator/*.ts.in"
      ],
      "extensions": [
        "js",
        "in"
      ]
    },
    "build:wasm:debug": {
      "patterns": [
        "build/core.ts"
      ],
      "extensions": "ts"
    },
    "build:js": {
      "patterns": [
        "src/lib/jsspeccy/*.js",
        "src/lib/jsspeccy/icons/*.svg",
        "build/*.js"
      ],
      "extensions": [
        "js",
        "svg"
      ]
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)(\\?inline)?$": "jest-transform-stub"
    },
    "globals": {
      "STAGING_ENV": "prod"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
